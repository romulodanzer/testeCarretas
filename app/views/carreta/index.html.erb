<p id="notice"><%= notice %></p>

<h1>Carreta</h1>

<table>
  <thead>
    <tr>
      <th>Placa</th>
      <th>Tipo</th>
      <th>Tamanho</th>
      <th>Eixos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @carreta.each do |carretum| %>
      <tr>
        <td><%= carretum.placa %></td>
        <td><%= carretum.tipo %></td>
        <td><%= carretum.tamanho %></td>
        <td><%= carretum.eixos %></td>
        <td><%= link_to 'Show', carretum %></td>
        <td><%= link_to 'Edit', edit_carretum_path(carretum) %></td>
        <td><%= link_to 'Destroy', carretum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Carretum', new_carretum_path %>
